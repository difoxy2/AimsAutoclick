{
  "name": "AimsAutoclick",
  "version": "1.0",
  "description": "Choose Appl ID from list, takes you to Appl Sum!",
  "permissions": ["tabs", "activeTab", "declarativeContent", "storage"],    
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
  "options_page": "options.html",
  "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      }
    },  
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
],     
  "icons": {
      "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
  "manifest_version": 2
}
